import requests
import pyfiglet
print(pyfiglet.figlet_format('cyber baha'))
from rich import print

# Ask user for a URL
url = input("Enter the URL: ").strip()

try:
    response = requests.get(url, verify=False)
    if response.status_code == 200:
        print("[bold red on white]Technology Enumeration[/]  :")
        for key, value in response.headers.items():
            print(f"[bold yellow]{key}[/] : [bold green]{value}[/]")
    else:
        print(f"[bold red]Request failed[/] with status code: {response.status_code}")
except requests.exceptions.RequestException as e:
    print(f"[bold red]An error occurred:[/] {e}")
